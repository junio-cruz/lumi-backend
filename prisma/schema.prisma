datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Invoice {
  invoice_id               String @id
  customer_id              String
  reference_month          String
  electric_energy          Int
  scee_energy              Int
  compensated_energy       Int
  public_contribution_ilum Int
}

model Customer {
  customer_id String  @id
  email       String  @unique
  name        String?
}
